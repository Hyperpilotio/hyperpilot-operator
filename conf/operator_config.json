{
  "Operator": {
    "OutsideCluster": true,
    "LoadedControllers": [
      "SnapTaskController",
      "NodeSpecController"
    ]
  },
  "APIServer": {
    "Port": "8080",
    "Debug": true
  },
  "SnapTaskController": {
    "PluginsList": [
      {
        "Name": "snap-plugin-collector-prometheus",
        "Type": "collector",
        "Version": 1
      },
      {
        "Name": "influxdb",
        "Type": "publisher",
        "Version": 22
      }
    ],
    "ServiceList": [
      "resource-worker"
    ],
    "Analyzer": {
      "Enable": true,
      "Address": "localhost",
      "Port": 5000
    },
    "influxdb": {
      "host": "influxsrv.hyperpilot",
      "port": 8086,
      "database": "snap",
      "user": "root",
      "pasword": "hyperpilot",
      "https": false,
      "skip-verify": false
    },
    "CreateTaskRetry": 5,
    "PluginDownloadTimeoutMin": 5
  },
  "NodeSpecController": {
    "Image": "ogre0403/curl-metadata:type",
    "MongoDB": {
      "Host": [
        "127.0.0.1:27017"
      ],
      "Database": "machine",
      "Username": "",
      "Password": "",
      "Collection": "type"
    }
  }
}