{
  "Operator": {},
  "APIServer": {
    "Port": "8080",
    "Debug": false
  },
  "SnapTaskController": {
    "PluginsList": [
      {
        "Name": "snap-plugin-collector-prometheus",
        "Type": "collector",
        "Version": 1
      },
      {
        "Name": "influxdb",
        "Type": "publisher",
        "Version": 22
      }
    ],
    "ServiceList": [
      "resource-worker"
    ],
    "Analyzer": {
      "Enable": true,
      "Address": "localhost",
      "Port": 5000
    },
    "influxdb": {
      "host": "influxsrv.hyperpilot",
      "port": 8086,
      "database": "snap",
      "user": "root",
      "pasword": "hyperpilot",
      "https": false,
      "skip-verify": false
    },
    "CreateTaskRetry": 5,
    "PluginDownloadTimeoutMin": 5
  }
}